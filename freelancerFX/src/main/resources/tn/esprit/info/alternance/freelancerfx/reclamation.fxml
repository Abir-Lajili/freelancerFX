<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="tn.esprit.info.alternance.freelancerfx.ReclamationController">
    <TextField fx:id="nom" promptText="Your Full Name"/>
    <TextField fx:id="email" promptText="Your Email"/>
    <TextArea fx:id="message" promptText="Your Message"/>
    <Button text="Envoyer Reclamation" onAction="#envoyerReclamation"/>
    <Button text="Annuler" onAction="#annuler"/>
    <Label fx:id="idMsg"/>
</VBox>